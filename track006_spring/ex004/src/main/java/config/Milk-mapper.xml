<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swc.dao.MilkDao">



  <insert  id="insert"  parameterType="com.swc.dto.MilkDto">
  
  insert into milk(mno,mname,mnum,mtotal) 
            values(milk_seq.nextval,  #{mname}   , #{mnum}, #{mtotal}  )
 
  </insert>
  
  <select resultType="MilkDto"  id="selectAll">
  select * from milk order by mno desc
  
  </select>
  
  <select resultType="MilkDto"  id="select"  parameterType="int">
  select * from milk where mno = #{mno}
  </select>
  
  <update id="update"  parameterType="MilkDto">
  update milk 
  set mname=#{mname}, mnum = #{mnum}, mtotal=#{mtotal}
  where mno=#{mno}
  </update>
  
  
  <delete id="delete" parameterType="int">
  delete from milk where mno=#{mno}
  </delete>
</mapper>